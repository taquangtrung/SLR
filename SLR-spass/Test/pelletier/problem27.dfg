begin_problem(Pelletier27).

list_of_descriptions.
name({*Pelletier's Problem No. 27*}).
author({*Christoph Weidenbach*}).
status(unsatisfiable).
description({*Problem taken in revised form from the "Pelletier Collection", Journal of Automated
	Reasoning, Vol. 2, No. 2, pages 191-216*}).
end_of_list.

list_of_symbols.
  predicates[(F,1), (G,1), (H,1), (J,1), (I,1)].
end_of_list.

list_of_formulae(axioms).

formula(exists([U],and(F(U),not(G(U))))).
formula(forall([U],implies(F(U),H(U)))).
formula(forall([U],implies(and(J(U),I(U)),F(U)))).
formula(implies(exists([U],and(H(U),not(G(U)))),forall([V],implies(I(V),not(H(V)))))).
end_of_list.

list_of_formulae(conjectures).

formula(forall([U],implies(J(U),not(I(U))))).

end_of_list.

end_problem.

