begin_problem(Pelletier49).

list_of_descriptions.
name({*Pelletier's Problem No. 49*}).
author({*Christoph Weidenbach*}).
status(unsatisfiable).
description({*Problem taken in revised form from the "Pelletier Collection", Journal of Automated
	Reasoning, Vol. 2, No. 2, pages 191-216*}).
end_of_list.

list_of_symbols.
  functions[(a,0), (b,0)].
  predicates[(P,1)].
end_of_list.

list_of_formulae(axioms).

formula(exists([U,V],forall([W],or(equal(W,U),equal(W,V))))).
formula(and(P(a),P(b))).
end_of_list.

list_of_formulae(conjectures).

formula(or(equal(a,b),forall([U],P(U)))).

end_of_list.

end_problem.

